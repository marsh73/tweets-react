{
  "name": "marsh-tweet-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run copy:fonts && npm run copy:index && npm run sass && webpack",
    "clean": "rimraf build/*",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build",
    "sass": "sass src/main.scss:build/main.css",
    "watch:sass": "watch 'src/*.scss' -c 'npm run sass'",
    "watch:js": "watch 'src/*.js' -c 'webpack'",
    "watch:jsx": "watch 'src/*.jsx' -c 'webpack'",
    "watch:webpack": "webpack --watch",
    "copy:fonts": "copyfiles -f node_modules/bootstrap-sass/assets/fonts/bootstrap/**/*.{ttf,woff,woff2,eof,svg} build/fonts/bootstrap ",
    "copy:index": "copyfiles -u 1 src/index.html build",
    "start": "npm run build && concurrently  \"npm run watch:webpack\" \"npm run dev\" \"node app.js\" ",
    "webpack": "webpack"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap-sass": "^3.3.6",
    "classnames": "^2.2.3",
    "es6-promise": "^3.1.2",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "body-parser": "^1.15.0",
    "concurrently": "^2.0.0",
    "copyfiles": "^0.2.1",
    "express": "^4.13.4",
    "react-marked": "^0.3.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.4",
    "redux-logger": "^2.6.0",
    "rimraf": "^2.5.2",
    "sass": "^0.5.0",
    "twitter": "^1.2.5",
    "watch-cli": "^0.2.1",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.7.1"
  }
}
