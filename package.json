{
  "name": "marsh-tweet-react",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run copy:fonts && npm run copy:index && npm run sass && webpack",
    "clean": "rimraf build/*",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot ",
    "sass": "sass src/main.scss:build/main.css",
    "watch:sass": "watch 'src/*.scss' -c 'npm run sass'",
    "watch:js": "watch 'src/*.js' -c 'webpack'",
    "watch:jsx": "watch 'src/*.jsx' -c 'webpack'",
    "watch:webpack": "webpack --watch",
    "copy:fonts": "copyfiles -f node_modules/bootstrap-sass/assets/fonts/bootstrap/**/*.{ttf,woff,woff2,eof,svg} build/fonts/bootstrap ",
    "copy:index": "copyfiles -u 1 src/index.html build",
    "start": "npm run build && concurrently  \"npm run watch:webpack\" \"node server.js\" \"node app.js\" \"npm run test\" ",
    "webpack": "webpack",
    "test": "karma start karma.conf.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bootstrap-sass": "^3.3.6",
    "classnames": "^2.2.3",
    "es6-promise": "^3.1.2",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.1",
    "material-ui": "^0.14.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "scroll-behavior": "^0.3.3",
    "webpack": "^1.12.14"
  },
