    "babel-core": "^6.5.2",
    "babel-istanbul": "^0.6.0",
    "babel-istanbul-loader": "0.0.3",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-runtime": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-survivejs-kanban": "^0.3.3",
    "babel-runtime": "^6.5.0",
    "body-parser": "^1.15.0",
